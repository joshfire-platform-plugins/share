{
  "name": "share",
  "fullName": "Share",
  "version": "0.1.2",
  "keywords": [
    "share",
    "sharekit"
  ],
  "description": "Share via Mail, Facebook, Twitter",
  "joshfire": {
    "devices": [
      "phone-ios",
      "phone-android",
      "tablet-ios",
      "tablet-android"
    ],
    "hooks": [
      "plugman",
      "xcodeproj",
      "share"
    ],
    "options": {
      "schema": {
        "app-url": {
          "type": "string",
          "title": "Application URL"
        },
        "fb-app-key": {
          "type": "string",
          "title": "Facebook app key",
          "description": "only used for iOS5 deploys. if your deploy target is iOS, Facebook Share is handled by the native Facebook iOS account integration."
        }
      }
    }
  }
}
